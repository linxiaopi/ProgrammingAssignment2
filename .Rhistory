## Write a short comment describing this function
makeCacheMatrix <- function(x = matrix()) {
Inv <- NULL
set <- function(y) {
x <<- y
Inv <<- NULL
}
get <- function() x
setI <- function(inv) {
Inv <<- inv
}
getI <- function() Inv
list(set = set, get = get, setI = setI, getI = getI)
}
## Write a short comment describing this function
cacheSolve <- function(x, ...) {
Inv <- x$getI()
if(!is.null(Inv)) {
message("Getting cached data")
return(Inv)
}
data <- x$get()
Inv <- solve(data, ...)
x$setI(Inv)
Inv
}
a <- matrix(10:13,2,2)
solve(a)
b <- makeCacheMatrix(a)
b
cacheSolve(b)
